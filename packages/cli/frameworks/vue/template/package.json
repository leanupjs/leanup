{
  "name": "@template/vue",
  "version": "1.0.60",
  "description": "",
  "scripts": {
    "e2e-test": "mkdirp .reports && cucumber-js tests/cucumber/features/**/*.feature --require cucumber.conf.js --require tests/cucumber/step-definitions --format node_modules/cucumber-pretty --format json:.reports/cucumber.json",
    "build": "vue build --gzip",
    "coverage": "vue coverage",
    "e2e": "vue e2e",
    "reinstall": "npm run remove && npm run update",
    "remove": "rm -rf node_modules package-lock.json",
    "serve": "vue serve",
    "start": "npm run serve -- --open",
    "test": "vue test",
    "update": "npm install && npm update"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{src,tests}/**/*.{html,js,json,jsx,ts,tsx,gql,graphql}": [
      "eslint"
    ],
    "{src,tests}/**": [
      "prettier --check"
    ]
  },
  "dependencies": {
    "@leanup/form": "^1.0.60",
    "@leanup/lib": "^1.0.60",
    "@vue/composition-api": "^0.6.7",
    "navigo": "^7.1.2",
    "rxjs": "^6.6.3",
    "vue": "^2.0.0",
    "vue-template-compiler": "^2.0.0"
  },
  "devDependencies": {
    "@leanup/cli": "^1.0.60",
    "@leanup/cli-addons": "^1.0.60",
    "@leanup/cli-vue": "^1.0.60",
    "@leanup/git-hooks": "^1.0.60",
    "chromedriver": "^86.0.0",
    "geckodriver": "^1.20.0",
    "selenium-server": "^3.141.59",
    "typescript": "^4.0.5"
  },
  "gitHead": "d77d4ffcfdbaddda7b135c14a94a809981d488a6"
}
