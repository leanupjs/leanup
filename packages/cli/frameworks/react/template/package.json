{
  "name": "@template/react",
  "version": "1.0.17",
  "description": "",
  "scripts": {
    "e2e-test": "mkdirp .reports && cucumber-js tests/cucumber/features/**/*.feature --require cucumber.conf.js --require tests/cucumber/step-definitions --format node_modules/cucumber-pretty --format json:.reports/cucumber.json",
    "build": "react build --gzip",
    "coverage": "react coverage",
    "e2e": "react e2e",
    "reinstall": "npm run remove && npm run update",
    "remove": "rm -rf node_modules package-lock.json",
    "serve": "react serve",
    "start": "npm run serve -- --open",
    "test": "react test",
    "update": "npm install && npm update"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{src,tests}/**/*.{html,js,json,jsx,ts,tsx,gql,graphql}": [
      "eslint"
    ],
    "{src,tests}/**": [
      "prettier"
    ]
  },
  "dependencies": {
    "@leanup/form": "^1.0.17",
    "@leanup/lib": "^1.0.17",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "navigo": "^7.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@leanup/cli": "^1.0.17",
    "@leanup/cli-addons": "^1.0.17",
    "@leanup/cli-react": "^1.0.17",
    "@leanup/git-hooks": "^1.0.17",
    "chromedriver": "^85.0.1",
    "geckodriver": "^1.20.0",
    "selenium-server": "^3.141.59",
    "typescript": "^4.0.3"
  },
  "gitHead": "d77d4ffcfdbaddda7b135c14a94a809981d488a6"
}
